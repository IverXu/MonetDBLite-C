# >Description:
# In the following script the string becomes different
# in the inner procedure (when the str module is loaded):
# 
# Script
# 8<-----
# PROC test_inner(in) := {
#    printf("test_inner.in: %s\n",str(in));
# }
#  
# PROC test_outer(in) := {
#    printf("test_outer.in: %s\n",str(in));
#    test_inner(in);
# }
# 8<-----
# Output
# 8<-----
# >test_outer("chr");
# test_outer.in: chr
# test_inner.in: chr
# >module(str);
# >test_outer("chr");
# test_outer.in: chr
# test_inner.in: test_inner.in: %s
# 8<----

setoid(oid(20000000));

PROC test_inner(in) := {
   printf("test_inner.in: %s\n",str(in));
}
 
PROC test_outer(in) := {
   printf("test_outer.in: %s\n",str(in));
   test_inner(in);
}

test_outer("chr");
test_outer.in: chr
test_inner.in: chr
module(str);
test_outer("chr");
test_outer.in: chr
test_inner.in: chr

quit;
