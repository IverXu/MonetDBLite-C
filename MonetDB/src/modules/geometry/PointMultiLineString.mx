@f PointMultiLineString
@A C.W. Quak
@h
#ifndef __POINTMULTILINESTRING_H__
#define __POINTMULTILINESTRING_H__

extern bool contains(const MultiLineString& l,const Point& p);

#endif /* __POINTMULTILINESTRING_H__ */
@C
#include "geometry.h"
#include "opengis.h"

bool contains(const MultiLineString& l,const Point& p)
{
   for(int i=0;i<l.nitems();++i)
      if(contains(l.geometry(i),p))
         return(true);
   return(false);
}

